<div class="row">
  <div class="col-md-9">
    <div class="row">
        <% @jobs.each do |job| %>
          <div class="col-md-6">
            <%= render job %>
          </div>
        <% end %>
    </div>
  </div>

  <div class="col-md-3">
  <aside>
    <div class="panel panel-default">
      <div class="panel panel-heading">
        <h1 class="panel-title"> Empresas </h1>
      </div>
      <div class="panel panel-body">
        <ul class="list-unstyled">
          <% @companies.each do |company| %>
            <li> <%= link_to "#{company.name}", company %> </li>
          <%end %>
        </ul>
      </div>
    </div>
  </aside>
  </div>

</div>
